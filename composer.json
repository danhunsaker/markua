{
    "name": "danhunsaker/markua",
    "type": "library",
    "description": "Markdown parser for PHP based on the Markua spec",
    "keywords": ["markdown","parser","markua"],
    "homepage": "https://github.com/danhunsaker/markua",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Davey Shafik",
            "email": "me@daveyshafik.com",
            "homepage": "http://daveyshafik.com",
            "role": "Creator"
        },
        {
            "name": "Dan Hunsaker",
            "email": "danhunsaker+markua@gmail.com",
            "homepage": "http://danhunsaker.awswan.com",
            "role": "Maintainer"
        }
    ],
    "require": {
        "php": "^5.6.5|^7.0",
        "league/commonmark": "^0.17.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.3",
        "codeclimate/php-test-reporter": "~0.1"
    },
    "autoload": {
        "psr-4": {
            "Danhunsaker\\Markua\\": "src/"
